{
  "name": "console-panel",
  "main": "./lib/console",
  "version": "0.3.1",
  "description": "Read only log console panel",
  "keywords": [],
  "repository": "https://github.com/spark/console-panel",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "console-panel": {
      "description": "Read only log console",
      "versions": {
        "1.0.0": "provideConsolePanel"
      }
    }
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "activationCommands": {
    "atom-workspace": [
      "console:toggle"
    ]
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "scripts": {
    "build": "rollup --config",
    "clean": "rimraf ./lib",
    "lint": "eslint --cache --fix",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "rollup --watch --config"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0",
    "event-kit": "^2.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "eslint": "^7.12.1",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-json": "^2.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint"
  }
}
